{
  "name": "apitte/presenter",
  "description": "ApiPresenter for Nette framework",
  "keywords": [
    "nette",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/presenter",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "apitte/core": "~0.7",
    "nette/application": "~3.0.0||~3.1.0",
    "nette/http": "~3.0.1||~3.1.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Presenter\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
